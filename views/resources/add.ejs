<% include ../partials/header.ejs %>

<form style="width: 30%; margin: 25px auto;" action="/resources/add" method="POST">
  <h1 class="title">Add Resources</h1>
  <div class="form-group">
    <label >Resource Name: </label>
    <input class="form-control" type="text" name="name" placeholder="Name">
  </div>
  <div class="form-group">
    <label>Resource Quantity: </label>
    <input class="form-control" type="Number" name="quantity" placeholder="Quantity"><br>
  </div>
  <div class="form-group">
    <button class="btn btn-large btn-primary btn-block">Submit</button>
  </div>
  
  
  
</form>
